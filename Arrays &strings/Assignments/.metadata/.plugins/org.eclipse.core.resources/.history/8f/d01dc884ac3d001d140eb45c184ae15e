#include "stdio.h"
void main(){
	int x[100];
	int k;
	int i;
	int a;
	int b;

	printf("Enter no of elements:");
	fflush(stdout); fflush(stdin);
	scanf("%d",k);

	printf("Enter the elements  :");
	fflush(stdout); fflush(stdin);
	scanf("%d",x);

	printf("Enter the elements to be searched  :");
	fflush(stdout); fflush(stdin);
	scanf("%d",a);

	for(i=0;sizeof(x);i++){
		if (a==x[i]){
			b=i+1;
		}
	}
	printf(" the element at the location  :%d",b);
}
