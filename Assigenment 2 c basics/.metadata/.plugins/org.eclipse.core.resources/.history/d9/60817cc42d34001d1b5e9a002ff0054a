#include "stdio.h"
void main(){
	int a;
	printf("Enter a number");
	fflush(stdout);fflush(stdin);
	scanf("%d",&a);
	if(a>0){printf("%d is positive",a);}
	else if(a<0) {printf("%d is negative",a);}
	else printf("avoid number 0");
}

