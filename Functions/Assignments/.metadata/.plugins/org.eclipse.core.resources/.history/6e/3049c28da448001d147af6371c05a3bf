
#include "stdio.h"

int check_prime (int num);

int main (){
	int i,n1,n2,flag;
	printf("Enter two numbers");
	fflush(stdout); fflush(stdin);
	scanf("%d%d",&n1,&n2);
	printf("prime numbers between %d and %d are",n1,n2);
	for(i=n1;i<n2;i++){
		flag= check_prime(i);
		if (flag==1)
			printf("%d\t",i);
	}
}

int check_prime(int num){
	int flag=0;
	int j;
	for (j=2;j<=num/2;j++){
		if(num%j==0)
				flag=1;
		else
			flag=0;
	}

	return flag;
}

