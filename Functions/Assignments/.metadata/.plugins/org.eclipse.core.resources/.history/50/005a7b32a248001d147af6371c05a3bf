
#include "stdio.h"

int check_prime (int num);

int main (){
	int i,n1,n2,flag;
	printf("Enter two numbers");
	fflush(stdout); fflush(stdin);
	scanf("%d%d",n1,n2);
	printf("prime numbers between %d and %d are",n1,n2);
	for(i=n1;i=n2;i++){
		int check_prime(int i);
		if (flag==1)
			printf("%d",i);
	}
}

int check_prime(int num){
	int flag=0;
	if(num%2==0)
		flag=0;
	else
		flag=1;
	return flag;
}

