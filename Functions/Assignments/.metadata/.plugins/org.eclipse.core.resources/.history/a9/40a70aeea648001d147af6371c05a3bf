#include "stdio.h"

int fac (int k){
	int f=1;
	f=f*k;
	--k;
	return f;
}

int main(){
	int f;
	int k;
	int i;
	printf("Enter a positive number ");
	fflush(stdout); fflush(stdin);
	scanf("%d",&k);
	for(i=1;i<k;i++){
		f=fac(k);
	}
	printf("factorial of %d is %d",k,f);

}

