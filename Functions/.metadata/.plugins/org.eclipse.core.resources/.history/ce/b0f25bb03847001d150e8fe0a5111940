#include "stdio.h"

int prime (int x);
int a,b;

int main(){


	printf("Enter two numbers:\t");
	fflush(stdout); fflush(stdin);
	scanf("%d%d",&a,&b);

	int prime(int x);

}

int prime (int x){


	for(x=a;x<=b;x++){
		if(x%2==0){
			x++;
		}
		else{
			printf("%d are prime numbers",x);
			x++;
		}
	}
	return x;
}
