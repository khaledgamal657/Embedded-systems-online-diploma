#include "stdio.h"

int prime (int x);
int a,b;

int main(){


	printf("Enter two numbers:\t");
	fflush(stdout); fflush(stdin);
	scanf("%d%d",&a,&b);

	int prime(int x);

}

int prime (int x){
	int i;

	for(i=a;i<=b;i++){
		if(i%2==0){
			x++;
		}
		else{
			printf("%d are prime numbers",i);
			x++;
		}
	}

}
